programa {
  inteiro x = 40, y = 100

  funcao vazio meio() {
    inteiro x = 10, y = 20
    escreva("x_ = ", x, "\n")
    escreva("y_ = ", y, "\n")
  }

  funcao vazio pula() {
    escreva("--\n")
  }

  funcao inicio() {
    inteiro x = 2
    escreva("x = ", x, "\n")
    escreva("y = ", y, "\n")
    pula()
    meio()
    pula()
    escreva("x = ", x, "\n")
    escreva("y = ", y, "\n")
  }
}
